<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.demo.iot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTranslucentTheme">
        <activity
            android:name=".HomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".SettingsActivity">
        </activity>
        <activity android:name=".StatisticsActivity">
        </activity>
        <activity android:name=".StatisticsAirQualityActivity">
        </activity>
        <activity android:name=".StatisticsPowerConsumptionActivity">
        </activity>
        <activity android:name=".StatisticsFilterRemainsActivity">
        </activity>
    </application>

</manifest>